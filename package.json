{
  "name": "@touched/react-three-gba",
  "version": "0.0.1",
  "description": "Renders GBA graphics using THREE.js and React",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ext .js --ignore-path .gitignore --ignore-pattern dist --cache",
    "start": "webpack-dev-server",
    "build": "babel src/ --out-dir lib/ --ignore __tests__,__mocks__",
    "clean": "rimraf lib",
    "lint": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "check": "flow check",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "gba",
    "threejs",
    "react"
  ],
  "author": "Touched",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "cross-env": "^3.1.4",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.40.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "jest": "^19.0.2",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "dependencies": {
    "react-dimensions": "^1.3.0",
    "react-motion": "^0.4.7",
    "react-three-renderer": "^3.0.2",
    "three": "^0.84.0"
  }
